{
  "name": "mcp-gemini-server",
  "version": "0.1.0",
  "description": "My new MCP Server",
  "main": "dist/server.js",
  "bin": {
    "mcp-gemini-server": "dist/initialize.js"
  },
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm src/server.ts\"",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "node --test --experimental-test-module-mocks --loader ts-node/esm tests/**/*.test.ts",
    "test:unit": "node --test --experimental-test-module-mocks --loader ts-node/esm tests/unit/**/*.test.ts",
    "test:github": "node --test --experimental-test-module-mocks --loader ts-node/esm tests/unit/services/gemini/GitHubApiService.test.ts",
    "test:integration": "node --test --experimental-test-module-mocks --loader ts-node/esm tests/integration/**/*.test.ts",
    "test:e2e": "node --test --experimental-test-module-mocks --loader ts-node/esm tests/e2e/**/*.test.ts",
    "prepare": "husky install || true"
  },
  "keywords": [
    "mcp",
    "model-context-protocol"
  ],
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^0.10.0",
    "@modelcontextprotocol/sdk": "1.7.0",
    "@octokit/graphql": "^8.2.2",
    "@octokit/rest": "^21.1.1",
    "@types/inquirer": "^9.0.7",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.3.0",
    "gitdiff-parser": "^0.3.1",
    "inquirer": "^12.5.0",
    "keyv": "^5.3.3",
    "micromatch": "^4.0.8",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
